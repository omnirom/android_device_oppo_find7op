import init.oppo.common.rc
import init.qcom.power.rc

on init
    # NXP HAL expects /dev/pn54x
    symlink /dev/pn544 /dev/pn54x
    chown nfc nfc /dev/pn54x

service mpdecision /system/vendor/bin/mpdecision --avg_comp
    class main
    user root
    group root readproc
    disabled
